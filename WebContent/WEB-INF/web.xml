<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>ClientAdmin</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <servlet-name>home</servlet-name>
    <servlet-class>frd.dds.servlet.Index</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>home</servlet-name>
    <url-pattern>/index</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>addClient</servlet-name>
    <servlet-class>frd.dds.servlet.abm.AddClient</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>addClient</servlet-name>
    <url-pattern>/addClient</url-pattern>
  </servlet-mapping>

</web-app>